@model Survey

@{
    ViewBag.Title = "Apklausa";
    Layout = "_Layout";
}

<div class="main-content-header container-fluid d-flex d-row justify-content-between align-items-center p-2">
    <span class="title">
        <h1>@ViewBag.Title</h1>
    </span>
    <span>
        <a class="btn btn-outline-primary" asp-controller="Survey" asp-action="ShowSurveyList">Grįžti į apklausų sąrašą</a>
    </span>
</div>
